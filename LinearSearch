#include <stdio.h>
int main()
{
int a[100],n,i,key,index;
printf("Enter the number of elements :- ");
scanf("%d",&n);
printf("Enter the elements :- ");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("Enter the element to search :- ");
scanf("%d",&key);
index=-1;
for(i=0;i<n;i++)
{
if(a[i]==key)
{
index=i;
break;
}
}
if(index==-1)
{
printf("Element is not found\n");
}
else
{
printf("Element is found at index %d\n",index);
}
return 0;
}
